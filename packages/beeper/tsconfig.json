{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"forceConsistentCasingInFileNames": true,
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "Bundler",
		"outDir": "./dist",
		"strict": true,
		"skipLibCheck": true,
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true
	}
}
